{
	"version": 2,
	"settings": {
		"color": {
			"custom": true,
			"customDuotone": true,
			"customGradient": true,
			"duotone": [],
			"gradients": [],
			"link": false,
			"palette": [
				{
					"name": "Base",
					"slug": "base",
					"color": "#fff"
				},
				{
					"name": "Contrast",
					"slug": "contrast",
					"color": "#2c2e31"
				},
				{
					"name": "Primary",
					"slug": "primary",
					"color": "#f3713c"
				}
			],
			"text": true,
			"background": true,
			"defaultGradients": false,
			"defaultPalette": false
		}
	},
	"styles": {
		"color": {
			"background": "var(--wp--preset--color--base)",
			"text": "var(--wp--preset--color--contrast)"
		},
		"elements": {
			"h2": {
				"color": {
					"text": "var(--wp--preset--color--primary)"
				}
			},
			"button": {
				":hover": {
					"color": {
						"background": "var(--wp--preset--color--primary)"
					}
				},
				":focus": {
					"color": {
						"background": "var(--wp--preset--color--primary)"
					}
				},
				":active": {
					"color": {
						"background": "var(--wp--preset--color--primary)"
					}
				}
			},
			"link": {
				":hover": {
					"color": {
						"text": { "ref": "styles.color.text" }
					}
				},
				":focus": {
					"color": {
						"text": { "ref": "styles.color.text" }
					}
				},
				":active": {
					"color": {
						"text": { "ref": "styles.color.text" }
					}
				}
			}
		},
		"blocks": {
			"core/heading": {
				"elements": {
					"h3": {
						"color": {
							"text": { "ref": "styles.color.text" }
						}
					}
				}
			}
		}
	}
}
